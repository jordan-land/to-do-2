{
    "name": "jordanpotts/php-todo-list",
    "type": "project",
    "autoload": {
        "psr-4": {
            "PhpTodoList\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "doctrine/dbal": "^3.6",
        "nikic/fast-route": "^1.3",
        "php-di/php-di": "^7.0",
        "symfony/var-dumper": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
